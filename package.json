{
    "name": "kashmir-bnb-server",
    "version": "1.0.0",
    "description": "Kashmir Tourism Backend API - Weather, destinations, and POI management",
    "main": "build/app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon app.ts",
        "build": "tsc",
        "start": "node build/app.js",
        "start:prod": "NODE_ENV=production node build/app.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "health": "node -e \"require('http').get('http://localhost:3000/api/health', (res) => { console.log('Health check:', res.statusCode); process.exit(res.statusCode === 200 ? 0 : 1) }).on('error', (err) => { console.log('Health check failed:', err.message); process.exit(1); })\""
    },
    "keywords": [
        "kashmir",
        "tourism",
        "api",
        "weather",
        "destinations",
        "nodejs",
        "typescript"
    ],
    "author": "KashmirBNB Team",
    "license": "ISC",
    "type": "commonjs",
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Flattr-io/kashmirBnB-server.git"
    },
    "bugs": {
        "url": "https://github.com/Flattr-io/kashmirBnB-server/issues"
    },
    "homepage": "https://github.com/Flattr-io/kashmirBnB-server#readme",
    "dependencies": {
        "@hyperflake/http-errors": "^1.0.0",
        "@supabase/supabase-js": "^2.57.4",
        "amadeus": "^11.0.0",
        "axios": "^1.12.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "geojson": "^0.5.0",
        "http-errors": "^2.0.0",
        "hyperflake": "^0.1.4",
        "moment": "^2.30.1",
        "moment-timezone": "^0.6.0",
        "morgan": "^1.10.1",
        "node-cron": "^4.2.1",
        "socket.io": "^4.8.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
